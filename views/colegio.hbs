                    <td>{{id_co}}</td>
                    <td>{{nombre_co}}</td>
                    <td>{{direccion_co}}</td>

                    <td>
                        <a href=""> Editar</a>
                        <a href=""> Borrar</a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>

