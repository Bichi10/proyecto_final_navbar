<body>
    <h1>Colegios</h1>
    <div class="container p 4">
        <table>
            <thead class="tabla">
                <th> colegios id</th>
                <th> colegios nombre</th>
                <th> direccion </th>
            </thead>
            <tbody>
                {{#each results}}
